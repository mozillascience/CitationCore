{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
        "max-len": "off",
        "no-param-reassign": "off",
        "no-nested-ternary": "off",
        "no-mixed-operators": "off",
        "no-restricted-syntax": "off",
        "brace-style": ["error", "stroustrup"],
        "no-unused-vars": ["error", { "args": "none" }],
	"no-underscore-dangle": ["error", { "allowAfterThis": true }]
    }
}
